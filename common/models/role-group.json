{
  "name": "RoleGroup",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": false
  },
  "mixins": {
    "UniqueConstraint": {
      "column": "name"
    },
    "TimeStamp": true,
    "UpdaterId": true
  },
  "properties": {
    "id": {
      "type": "number",
      "id": true,
      "generated": true,
      "description": "ID"
    },
    "name": {
      "type": "string",
      "required": true,
      "description": "role name"
    },
    "roleAdmin": {
      "type": "boolean",
      "default": false
    },
    "roleUser": {
      "type": "boolean",
      "default": false
    }
  },
  "validations": [],
  "relations": {},
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "roleAdmin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
