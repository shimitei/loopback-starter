{
  "name": "Account",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": false
  },
  "mixins": {
    "TimeStamp": true,
    "UpdaterId": true
  },
  "properties": {
    "id": {
      "type": "number",
      "id": true,
      "generated": true,
      "description": "ID"
    },
    "username": {
      "type": "string",
      "required": true,
      "description": "Login name"
    },
    "name": {
      "type": "string",
      "required": true,
      "description": "display name"
    },
    "password": {
      "type": "string",
      "required": true,
      "description": "Login password"
    },
    "email": {
      "type": "string",
      "required": false
    },
    "status": {
      "type": "number",
      "required": true,
      "description": "1:enable user 0:disable userï¼‰",
      "default": 1
    }
  },
  "validations": [],
  "relations": {
    "roleGroup": {
      "type": "belongsTo",
      "model": "RoleGroup"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "roleAdmin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
